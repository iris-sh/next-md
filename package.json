{
  "name": "next-md",
  "version": "0.0.12",
  "description": "minimalistic material design library with next.js in mind",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "$(npm bin)/lint-staged",
    "prepare": "npm run build",
    "prepublishOnly": "npm run deploy",
    "lint": "$(npm bin)/eslint src",
    "prettier": "$(npm bin)/prettier-standard \"src/**/*.js\"",
    "dev": "$(npm bin)/babel -w -d lib src",
    "build-lib": "rm -rf lib && $(npm bin)/babel -d lib src",
    "build-storybook": "rm -rf .out && build-storybook -c ./src/components/.storybook -o .out",
    "build": "npm run build-lib && npm run build-storybook",
    "start": "cd .out && serve -p 9001",
    "deploy": "now && now alias"
  },
  "lint-staged": {
    "./**/*.js": [
      "npm run lint",
      "npm run prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/next-md/next-md.git"
  },
  "homepage": "https://github.com/next-md/next-nd.git",
  "keywords": [
    "react",
    "material",
    "design",
    "next",
    "next.js"
  ],
  "author": "Tucker Connelly <web@tuckerconnelly.com>",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-info": "^3.2.0",
    "@storybook/addon-knobs": "^3.2.0",
    "@storybook/react": "^3.2.3",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "next": "^2.4.9",
    "prettier-standard": "^2.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "now": {
    "alias": "next-md"
  },
  "dependencies": {
    "color": "^1.0.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.3",
    "serve": "^6.0.6",
    "styled-jsx": "^0.5.7"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "styled-jsx/babel"
    ]
  }
}
