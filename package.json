{
  "name": "next-md",
  "version": "0.0.17",
  "description": "minimalistic material design library with next.js in mind",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "$(npm bin)/lint-staged",
    "prepare": "npm run build",
    "prepublishOnly": "npm run deploy",
    "lint": "$(npm bin)/eslint src",
    "prettier": "$(npm bin)/prettier-standard \"src/**/*.js\"",
    "dev": "$(npm bin)/babel -w -d lib src",
    "build-lib": "rm -rf lib && $(npm bin)/babel -d lib src",
    "build-storybook": "rm -rf .out && build-storybook -c ./src/components/.storybook -o .out",
    "build": "npm run build-lib && npm run build-storybook",
    "start": "cd .out && serve -p 9001",
    "dev-storybook": "start-storybook -p 9001  -c ./src/components/.storybook",
    "deploy": "now && now alias"
  },
  "lint-staged": {
    "./**/*.js": [
      "npm run lint",
      "npm run prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/next-md/next-md.git"
  },
  "homepage": "https://github.com/next-md/next-nd.git",
  "keywords": [
    "react",
    "material",
    "design",
    "next",
    "next.js"
  ],
  "author": "Tucker Connelly <web@tuckerconnelly.com>",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/addon-info": "^3.3.9",
    "@storybook/addon-knobs": "^3.3.9",
    "@storybook/addons": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "ajv": "^5.5.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "next": "^4.2.3",
    "prettier-standard": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "now": {
    "alias": "next-md"
  },
  "dependencies": {
    "color": "^2.0.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "serve": "^6.4.8",
    "styled-jsx": "^2.2.1",
    "transitionify": "0.0.5"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "styled-jsx/babel"
    ]
  }
}
