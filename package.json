{
  "name": "next-md",
  "version": "0.0.6",
  "description": "minimalistic material design library with next.js in mind",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "./node_modules/.bin/lint-staged",
    "prepublish": "yarn build",
    "lint": "./node_modules/.bin/eslint --fix src",
    "prettier": "./node_modules/.bin/prettier-standard \"src/**/*.js\"",
    "dev": "./node_modules/.bin/babel -w -d lib src",
    "build": "rm -rf lib && ./node_modules/.bin/babel -d lib src"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/next-md/next-md.git"
  },
  "homepage": "https://github.com/next-md/next-nd.git",
  "keywords": [
    "react",
    "material",
    "design",
    "next",
    "next.js"
  ],
  "author": "Tucker Connelly <web@tuckerconnelly.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier-standard": "^2.0.2",
    "react": "^15.5.4"
  },
  "dependencies": {
    "color": "^1.0.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.3",
    "styled-jsx": "^0.5.7"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "styled-jsx/babel"
    ]
  }
}
